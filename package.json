{
  "name": "alkanes.build",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "sdkBranch": "develop",
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "pnpm build:docs && next build",
    "build:docs": "node scripts/build-docs.js",
    "build:external": "node build-external.js",
    "build:all": "pnpm build:external && pnpm build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "setup": "pnpm docker:up && pnpm db:push && pnpm build:external",
    "setup:local": "pnpm docker:up && sleep 3 && pnpm db:push",
    "gcp:setup": "bash ./gcp/setup.sh",
    "gcp:deploy": "bash ./gcp/deploy.sh",
    "gcp:setup-wif": "bash ./gcp/setup-github-wif.sh",
    "postinstall": "prisma generate"
  },
  "optionalDependencies": {
    "@alkanes/ts-sdk": "file:./ts-sdk"
  },
  "dependencies": {
    "@bitcoinerlab/secp256k1": "^1.2.0",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "^15.0.0",
    "@noble/hashes": "^2.0.1",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tanstack/react-query": "^5.83.0",
    "bignumber.js": "^9.3.1",
    "bitcoin-address-validation": "^3.0.0",
    "bitcoinjs-lib": "^7.0.0",
    "bitcoinjs-message": "^2.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "ecpair": "^3.0.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.396.0",
    "marked": "^14.0.0",
    "next": "^15.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype-highlight": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "tiny-secp256k1": "^2.2.4",
    "varuint-bitcoin": "^2.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "bip39": "^3.1.0",
    "eslint": "^8",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.47",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5"
  }
}
